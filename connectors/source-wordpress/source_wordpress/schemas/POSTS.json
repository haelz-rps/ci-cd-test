                  "format": "uri"
                }
              },
              "required": [
                "count",
                "href"
              ]
            }
          },
          "predecessor-version": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "href": {
                  "type": "string",
                  "format": "uri"
                }
              },
              "required": [
                "id",
                "href"
              ]
            }
          },
          "wp:attachment": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "href": {
                  "type": "string",
                  "format": "uri"
                }
              },
              "required": [
                "href"
              ]
            }
          },
          "wp:term": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "taxonomy": {
                  "type": "string"
                },
                "embeddable": {
                  "type": "boolean"
                },
                "href": {
                  "type": "string",
                  "format": "uri"
                }
              },
              "required": [
                "taxonomy",
                "embeddable",
                "href"
              ]
            }
          },
          "curies": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "href": {
                  "type": "string",
                  "format": "uri-template"
                },
                "templated": {
                  "type": "boolean"
                }
              },
              "required": [
                "name",
                "href",
                "templated"
              ]
            }
          }
