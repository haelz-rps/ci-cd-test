              "type": "object",
              "properties": {
                "href": {
                  "type": "string",
                  "format": "uri"
                },
                "targetHints": {
                  "type": "object",
                  "properties": {
                    "allow": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  },
                  "required": [
                    "allow"
                  ]
                }
              },
              "required": [
                "href",
                "targetHints"
              ]
            }
          },
          "collection": {
            "type": "array",
