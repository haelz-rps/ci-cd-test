    "required": [
      "id",
      "date",
      "date_gmt",
      "guid",
      "modified",
      "modified_gmt",
      "slug",
      "status",
      "type",
      "link",
      "title",
      "content",
      "excerpt",
      "author",
      "featured_media",
      "comment_status",
      "ping_status",
      "sticky",
      "template",
      "format",
      "meta",
      "categories",
      "tags",
      "class_list",
      "_links"
    ]
